shader test() {
    bool b1 = true, b2 = false;
    int i1 = 2, i2 = 2;
    float f1 = 2, f2 = 2;
    color c1 = 2, c2 = 2;
    point p1 = 2, p2 = 2;
    normal n1 = 2, n2 = 2;
    vector v1 = 2, v2 = 2;
    matrix m1 = 2, m2 = 2;
    closure color k1, k2;

    // Booleans
    { bool b = !b1; }
    { bool b = ~b1; }
    { bool b = b1 & b2; }
    { bool b = b1 | b2; }
    { bool b = b1 && b2; }
    { bool b = b1 || b2; }
    { bool b = b1 < b2; }
    { bool b = b1 > b2; }
    { bool b = b1 <= b2; }
    { bool b = b1 >= b2; }
    { bool b = b1 == b2; }
    { bool b = b1 != b2; }

    // Floating-point numbers
    { float f = f1++; }
    { float f = f1--; }
    { float f = ++f1; }
    { float f = --f1; }
    { float f = -f1; }
    { float f = f1 * f2; }
    { float f = f1 / f2; }
    { float f = f1 + f2; }
    { float f = f1 - f2; }
    { bool b = f1 < f2; }
    { bool b = f1 > f2; }
    { bool b = f1 <= f2; }
    { bool b = f1 >= f2; }
    { bool b = f1 == f2; }
    { bool b = f1 != f2; }

    // Integers
    { int i = i1++; }
    { int i = i1--; }
    { int i = ++i1; }
    { int i = --i1; }
    { int i = -i1; }
    { int i = ~i1; }
    { int i = !i1; }

    { int i = i1 + i2; }
    { int i = i1 - i2; }
    { int i = i1 * i2; }
    { int i = i1 % i2; }
    { int i = i1 / i2; }
    { int i = i1 >> i2; }
    { int i = i1 << i2; }

    { bool b = i1 >= i2; }
    { bool b = i1 <= i2; }
    { bool b = i1 < i2; }
    { bool b = i1 > i2; }
    { bool b = i1 == i2; }
    { bool b = i1 != i2; }

    { bool b = i1 || i2; }
    { bool b = i1 && i2; }

    { int b = i1 & i2; }
    { int b = i1 ^ i2; }
    { int b = i1 | i2; }

    // Color
    { float f = c1[0]; }
    { float f = c1[1]; }
    { float f = c1[2]; }
    { color c = -c1; }
    { color c = f1 * c2; }
    { color c = c1 * f2; }
    { color c = c1 * c2; }
    { color c = c1 / c2; }
    { color c = c1 + c2; }
    { color c = c1 - c2; }
    { bool b = c1 == c2; }
    { bool b = c1 != c2; }

    // Point
    { float f = p1[0]; }
    { float f = p1[1]; }
    { float f = p1[2]; }
    { point p = -p1; }
    { point p = f1 * p2; }
    { point p = p1 * f2; }
    { point p = p1 * p2; }
    { point p = p1 / p2; }
    { point p = p1 + p2; }
    { point p = p1 - p2; }
    { bool b = p1 == p2; }
    { bool b = p1 != p2; }

    // Vector
    { float f = v1[0]; }
    { float f = v1[1]; }
    { float f = v1[2]; }
    { vector v = -v1; }
    { vector v = f1 * v2; }
    { vector v = v1 * f2; }
    { vector v = v1 * v2; }
    { vector v = v1 / v2; }
    { vector v = v1 + v2; }
    { vector v = v1 - v2; }
    { bool b = v1 == v2; }
    { bool b = v1 != v2; }

    // Normal
    { float f = n1[0]; }
    { float f = n1[1]; }
    { float f = n1[2]; }
    { normal n = -n1; }
    { normal n = f1 * n2; }
    { normal n = n1 * f2; }
    { normal n = n1 * n2; }
    { normal n = n1 / n2; }
    { normal n = n1 + n2; }
    { normal n = n1 - n2; }
    { bool b = n1 == n2; }
    { bool b = n1 != n2; }

    // Matrix
    { float f = m1[0][0]; }
    { float f = m1[0][1]; }
    { float f = m1[0][2]; }
    { float f = m1[0][3]; }
    { float f = m1[1][0]; }
    { float f = m1[1][1]; }
    { float f = m1[1][2]; }
    { float f = m1[1][3]; }
    { float f = m1[2][0]; }
    { float f = m1[2][1]; }
    { float f = m1[2][2]; }
    { float f = m1[2][3]; }
    { float f = m1[3][0]; }
    { float f = m1[3][1]; }
    { float f = m1[3][2]; }
    { float f = m1[3][3]; }
    { matrix m = -m1; }
    { matrix m = f1 * m2; }
    { matrix m = m1 * f2; }
    { matrix m = m1 * m2; }
    { matrix m = m1 / m2; }
    { matrix m = m1 + m2; }
    { matrix m = m1 - m2; }
    { bool b = m1 == m2; }
    { bool b = m1 != m2; }

    // Closure color
    { closure color k = -k1; }
    { closure color k = c1 * k2; }
    { closure color k = k1 * c2; }
    { closure color k = f1 * k2; }
    { closure color k = k1 * f2; }
    { closure color k = k1 + k2; }
}
