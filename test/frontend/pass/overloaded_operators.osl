    { S s = s1 >> s2; }
    { bool b = s1 < s2; }
    { bool b = s1 > s2; }
    { bool b = s1 <= s2; }
    { bool b = s1 >= s2; }
    { bool b = s1 == s2; }
    { bool b = s1 != s2; }
    { S s = s1 & s2; }
    { S s = s1 ^ s2; }
    { S s = s1 | s2; }
    { ++s1; }
    { --s1; }
    { s1++; }
    { s1--; }
    { S s = -s1; }
    { S s = ~s1; }
    { S s = !s1; }

    { s1 *= s2; }
    { s1 /= s2; }
    { s1 %= s2; }
    { s1 += s2; }
    { s1 -= s2; }
    { s1 <<= s2; }
    { s1 >>= s2; }
